@model IEnumerable<View_Components_Ex.Models.Customer>
<table border="1">
    <tr>
        <td>Customer Id</td>
        <td>Customer Name</td>
        <td>Address</td>
        <td>Email Id </td>
        <td>Mobile No</td>
        <td>Creadit Limit</td>
    </tr>
    @foreach (var temp in Model)
    {
        <tr>
            <td>@temp.CustomerId</td>
            <td>@temp.CustomerName</td>
            <td>@temp.Address</td>
            <td>@temp.EmailId</td>
            <td>@temp.MobileNo</td>
            <td>@temp.CreaditLimit</td>
        </tr>
    }
</table>
